  "license": "MIT",
  "scripts": {
    "lint": "nx affected:lint --all",
    "format": "npx prettier --write .",
    "format:check": "nx format:check --all",
    "test": "nx affected:test --all",
    "prepare": "husky install",
    "generate:docs": "node scripts/build-readme-template.js",
    "dev:next": "nx serve next-app",
    "dev:docs": "nx serve docs",
    "dev:webhook": "nx serve webhook-server",
    "build:next-app": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 nx build next-app --prod",
    "build:webhook": "nx build webhook-server --prod",
    "build:docs": "npm run generate:docs && nx build docs --prod",
    "start:next": "cd dist/apps/next-app && npx next start",
    "start:webhook": "cd dist/apps/webhook-server && node main.js",
    "start:docs": "cd dist/apps/docs && npx next start"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "nx affected:lint"
    ]
  },
  "private": true,
