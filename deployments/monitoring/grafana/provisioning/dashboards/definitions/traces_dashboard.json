{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "8.3.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "datasource",
      "id": "tempo",
      "name": "Tempo",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "id": 2,
      "targets": [
        {
          "expr": "sum(rate(traces_spanmetrics_calls_total{service=~\"$service\", span_name=~\"$span\"}[$__range])) by (service, span_name)",
          "legendFormat": "{{service}} - {{span_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Request Error Rate",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "id": 4,
      "targets": [
        {
          "expr": "sum(rate(traces_spanmetrics_calls_total{service=~\"$service\", span_name=~\"$span\", status_code=\"STATUS_CODE_ERROR\"}[$__range])) by (service, span_name) / sum(rate(traces_spanmetrics_calls_total{service=~\"$service\", span_name=~\"$span\"}[$__range])) by (service, span_name)",
          "legendFormat": "{{service}} - {{span_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        }
      }
    },
    {
      "title": "Request Duration (p95)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "id": 6,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(traces_spanmetrics_latency_bucket{service=~\"$service\", span_name=~\"$span\"}[$__range])) by (le, service, span_name))",
          "legendFormat": "{{service}} - {{span_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "title": "Service Map",
      "type": "nodeGraph",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 16, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (client, server) (rate(traces_service_graph_request_total[$__rate_interval]))"
        }
      ],
      "options": {
        "edges": {
          "mainStat": "Value"
        },
        "nodes": {
          "mainStat": "Value"
        }
      }
    },
    {
      "title": "Recent Traces",
      "type": "table",
      "datasource": {
        "type": "tempo",
        "uid": "tempo_ds"
      },
      "gridPos": { "h": 16, "w": 24, "x": 0, "y": 24 },
      "id": 10,
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "tempo_ds"
          },
          "queryType": "traceql",
          "query": "{ resource.service.name=~\"$service\" && name=~\"$span\" }",
          "limit": 20,
          "tableType": "traces",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true,
        "footer": {
          "show": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          },
          "links": [
            {
              "title": "View Trace",
              "url": "/explore?left=%5B%22now-1h%22,%22now%22,%22tempo_ds%22,%7B%22query%22:%22${__data.fields.traceID}%22%7D%5D"
            }
          ]
        }
      }
    }
  ],
  "schemaVersion": 35,
  "style": "dark",
  "tags": ["tempo"],
  "templating": {
    "list": [
      {
        "current": {},
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(traces_spanmetrics_calls_total, service)",
        "hide": 0,
        "includeAll": true,
        "label": "Service",
        "multi": true,
        "name": "service",
        "options": [],
        "query": {
          "query": "label_values(traces_spanmetrics_calls_total, service)"
        },
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      },
      {
        "current": {},
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(traces_spanmetrics_calls_total{service=~\"$service\"}, span_name)",
        "hide": 0,
        "includeAll": true,
        "label": "Span",
        "multi": true,
        "name": "span",
        "options": [],
        "query": {
          "query": "label_values(traces_spanmetrics_calls_total{service=~\"$service\"}, span_name)"
        },
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "title": "Tempo Traces",
  "uid": "tempo-traces-dashboard",
  "version": 1
}
